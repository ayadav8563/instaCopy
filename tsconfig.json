{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "paths": {
      "_assets": ["src/assets/*"],
      "_components": ["src/components/index.ts"],
      "_navigation/*": ["src/navigation/*"],
      "_styles": ["src/styles/index.ts"],
      "_action": ["src/Redux/Actions/index.ts"],
      "_types": ["src/Redux/responseTypes/index.ts"],
      "_slices/*": ["src/Redux/Slices/*"],
      "_screens": ["src/screens/index.ts"],
      "_services": ["src/services/index.ts"],
      "_utils": ["src/utils/index.ts"],
      "_hooks": ["src/hooks/index.ts"],
      "_modals": ["src/modals/index.ts"],
    },
  },
  "target": "es6"
}
